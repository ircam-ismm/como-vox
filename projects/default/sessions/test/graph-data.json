{
  name: 'default',
  version: '0.0.0',
  modules: [
    {
      id: 'input',
      type: 'Input',
    },
    {
      id: 'motion-descriptors',
      type: 'MotionDescriptors',
    },
    {
      id: 'merge-descriptors',
      type: 'Merge',
    },
    {
      id: 'select-ml-data',
      type: 'ScriptData',
      options: {
        scriptName: 'ml-select-all',
      },
    },
    {
      id: 'ml-decoder',
      type: 'MLDecoder',
    },
    {
      id: 'beat-detection',
      type: 'ScriptData',
      options: {
        scriptName: 'kick',
      },
    },
    {
      id: 'constant',
      type: 'ScriptData',
      options: {
        scriptName: 'constant',
      },
    },
    {
      id: 'constant2',
      type: 'ScriptData',
      options: {
        scriptName: 'constant',
        // @TODO: initial parameters?
        name: 'constant2',
        value: 12,
      },
    },
    {
      // allow for direct connections
      // (in addition to connection from 'output')
      id: 'bridge-merge',
      type: 'Merge',
    },
    {
      id: 'bridge',
      type: 'Bridge',
    },
    {
      id: 'output',
      type: 'Output',
    },
    {
      id: 'full-stream',
      type: 'StreamRecorder',
      options: {
        name: 'full-stream/player',
      },
    },
  ],
  connections: [
    [
      'input',
      'merge-descriptors',
    ],
    [
      'input',
      'motion-descriptors',
    ],
    [
      'motion-descriptors',
      'merge-descriptors',
    ],
    [
      'merge-descriptors',
      'select-ml-data',
    ],
    [
      'select-ml-data',
      'ml-decoder',
    ],
    [
      'merge-descriptors',
      'beat-detection',
    ],
    [
      'input',
      'output',
    ],
    [
      'motion-descriptors',
      'output',
    ],
    [
      'ml-decoder',
      'output',
    ],
    [
      'beat-detection',
      'constant',
    ],
    [
      'beat-detection',
      'output',
    ],
    [
      'constant',
      'output',
    ],
    [
      // must connect to an input
      'input',
      'constant2',
    ],
    [
      'constant2',
      'output',
    ],
    [
      'output',
      'full-stream',
    ],
    [
      'output',
      'bridge-merge'
    ],
    [
      'bridge-merge',
      'bridge',
    ],
  ],
}
