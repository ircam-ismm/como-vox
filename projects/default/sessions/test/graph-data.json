{
  name: 'default',
  version: '0.0.0',
  modules: [
    {
      id: 'input',
      type: 'Input',
    },
    {
      id: 'timeTag',
      type: 'ScriptData',
      options: {
        scriptName: 'timeTag',
      },
    },
    {
      id: 'motion-descriptors',
      type: 'MotionDescriptors',
    },
    {
      id: 'merge-descriptors',
      type: 'Merge',
    },
    {
      id: 'select-ml-data',
      type: 'ScriptData',
      options: {
        scriptName: 'ml-select-all',
      },
    },
    {
      id: 'ml-decoder',
      type: 'MLDecoder',
    },
    {
      id: 'to-transport',
      type: 'Merge',
    },
    {
      id: 'transport',
      type: 'ScriptData',
      options: {
        scriptName: 'transport',
        scriptParams: {
          gestureControlsTempo: true,
        },
      },
    },
    {
      id: 'merge-descriptors-position',
      type: 'Merge',
    },
    {
      id: 'clickGenerator',
      type: 'ScriptData',
      options: {
        scriptName: 'clickGenerator',
        scriptParams: {
          onOff: false,
        },
      },
    },
    {
      id: 'score',
      type: 'ScriptData',
      options: {
        scriptName: 'score',
        scriptParams: {
          onOff: false,
        },
      },
    },
    {
      id: 'beatTriggerFromGesture',
      type: 'ScriptData',
      options: {
        scriptName: 'beatTriggerFromGesturePeakAdapt',
      },
    },
    {
      id: 'to-intensityFromGesture',
      type: 'Merge',
    },
    {
      id: 'intensityFromGesture',
      type: 'ScriptData',
      options: {
        scriptName: 'intensityFromGesture',
      },
    },
    {
      id: 'clackFromBeat',
      type: 'ScriptData',
      options: {
        scriptName: 'clackFromBeat',
        scriptParams: {
          onOff: true,
        },
      },
    },
    {
      id: 'output',
      type: 'Output',
    },
    {
      id: 'full-stream',
      type: 'StreamRecorder',
      options: {
        name: 'full-stream/player',
      },
    },
    {
      id: 'bridge',
      type: 'Bridge',
    },
  ],
  connections: [
    [
      'input',
      'output',
    ],
    [
      'input',
      'merge-descriptors',
    ],
    [
      'input',
      'timeTag',
    ],
    [
      'timeTag',
      'merge-descriptors',
    ],
    [
      'input',
      'motion-descriptors',
    ],
    [
      'motion-descriptors',
      'output',
    ],
    [
      'motion-descriptors',
      'merge-descriptors',
    ],
    [
      'merge-descriptors',
      'output',
    ],
    [
      'merge-descriptors',
      'select-ml-data',
    ],
    [
      'merge-descriptors',
      'to-transport',
    ],
    [
      'to-transport',
      'transport',
    ],
    [
      'merge-descriptors',
      'merge-descriptors-position',
    ],
    [
      'transport',
      'merge-descriptors-position',
    ],
    [
      'merge-descriptors',
      'beatTriggerFromGesture',
    ],
    [
      'select-ml-data',
      'ml-decoder',
    ],
    [
      'transport',
      'output',
    ],
    [
      'merge-descriptors-position',
      'clickGenerator',
    ],
    [
      'clickGenerator',
      'to-intensityFromGesture',
    ],
    [
      'merge-descriptors-position',
      'score',
    ],
    [
      'beatTriggerFromGesture',
      'clackFromBeat',
    ],
    [
      'beatTriggerFromGesture',
      'to-transport',
    ],
    [
      'beatTriggerFromGesture',
      'output',
    ],
    [
      'merge-descriptors-position',
      'to-intensityFromGesture',
    ],
    [
      'score',
      'to-intensityFromGesture',
    ],
    // do not connect score to output,
    // as it is filtered by intensity
    [
      'beatTriggerFromGesture',
      'to-intensityFromGesture',
    ],
    [
      'to-intensityFromGesture',
      'intensityFromGesture',
    ],
    [
      'intensityFromGesture',
      'output',
    ],
    [
      'clackFromBeat',
      'to-intensityFromGesture',
    ],
    [
      'ml-decoder',
      'output',
    ],
    [
      'output',
      'full-stream',
    ],
    [
      'output',
      'bridge',
    ],
  ],
